#define _CRT_SECURE_NO_WARNINGS 1

# include<stdio.h>

void ch_line(char c, int x, int y)
{
	int m, n;
	for (m = 0; m < x; m++)
	{
		for (n = 0; n < y; n++)
		{
			printf("%c", c);
		}
		printf("\n");
	}
	return 0;
}

int main()
{
	char ch;
	int i, j;
	printf("输入想打印的字符：");
	scanf("%c", &ch);
	printf("请输入想要打印的行数：");
	scanf("%d", &i);
	printf("请输入想要打印的行数：");
	scanf("%d", &j);
	ch_line(ch, i, j);
	return 0;
}
