#define _CRT_SECURE_NO_WARNINGS 1

# include<stdio.h>



int main()
{

	int i = 1;
	int j = 1;
	for (i = 1; i <= 4; i++)
	{
		for (j = 1; j <= 8; j++)
		{
			printf("$");
		}
		printf("\n");
	}
	return 0;
}




