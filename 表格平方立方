#define _CRT_SECURE_NO_WARNINGS 1

# include<stdio.h>
#include<string.h>

int main()
{
	int max ,min,i;
	printf("输入上限：");
	scanf("%d",&max);
	printf("输入下限：");
	scanf("%d", &min);
	printf("  数值：   平方：   立方：  \n");
	for (i= min; i <= max; i++)
	{
		printf("%6d%10d%10d\n", i, i * i, i * i * i);

	}
	return 0;
}
